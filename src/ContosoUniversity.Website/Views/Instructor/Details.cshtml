@model ContosoUniversity.Models.Instructor

@{
    ViewBag.Title = "Details";
}

<h2>Details</h2>

<div>
    <h4>Instructor</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt><label asp-for="OfficeAssignment.Location"></label></dt>
        <dd>@Model.OfficeAssignment.Location</dd>

        <dt><label asp-for="LastName"></label></dt>
        <dd>@Model.LastName</dd>

        <dt><label asp-for="FirstMidName"></label></dt>
        <dd>@Model.FirstMidName</dd>

        <dt><label asp-for="HireDate"></label></dt>
        <dd><display asp-for="HireDate" /></dd>
    </dl>
</div>
<p>
    <a asp-controller="Instructor" asp-action="Edit" asp-route-id=@Model.ID>Edit</a> |
    <a asp-controller="Instructor" asp-action="Index">Back to List</a>
</p>
