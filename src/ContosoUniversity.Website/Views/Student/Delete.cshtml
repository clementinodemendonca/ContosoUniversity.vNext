@model ContosoUniversity.Models.Student

@{
    ViewBag.Title = "Delete";
}

<h2>Delete</h2>
<p class="error">@ViewBag.ErrorMessage</p>
<h3>Are you sure you want to delete this?</h3>
<div>
    <h4>Student</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt><label asp-for="LastName"></label></dt>
        <dd>@Model.LastName</dd>

        <dt><label asp-for="FirstMidName"></label></dt>
        <dd>@Model.FirstMidName</dd>

        <dt><label asp-for="EnrollmentDate"></label></dt>
        <dd><display asp-for="EnrollmentDate" /></dd>
    </dl>

    <form asp-controller="Student" asp-action="Delete" method="post" role="form">
        <div class="form-actions no-color">
            <input type="submit" value="Delete" class="btn btn-danger" /> |
            <a asp-controller="Student" asp-action="Index">Back to List</a>
        </div>
    </form>
</div>

